/* Simpson Rule                    */
/***********************************/
a = 0.0;
b = 1.0;
h = 0.05;
pi = 3.1415922654;
q = 0.12;
x = a;
fun = sin(pi * x);
s1= fun;
while (x<b) {
  x = x + h;
  s1 = s1 + 4.0 * fun;
  x = x + h;
  fun = sin (pi * x);
  s1 = s1 + 2.0 * fun;
};
s1 = s1 + fun;
require_nsb(s1, 15);