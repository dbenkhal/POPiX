x = 0.0;
y = 1.0;
h = 0.7853981634;
n=4;
for(i=0.0;i<n+1.0;i=i+1.0;){
	x = x + i*h;
	f1 = h*(5.0*x*x - y)/exp(x + y);
	f2 = (h*(5.0*(x + h/2)*(x + h/2) - (y + f1/2)))/exp((x + h/2) + (y + f1/2));
	f3 = (h*(5.0*(x + h/2)*(x + h/2) - (y + f2/2)))/exp((x + h/2) + (y + f2/2));
	f4 = h*(5.0*(x+h)*(x+h) - (y+f3))/exp((x+h) + (y+f3));
	y = y + (f1 + 2*f2 + 2*f3 + f4)/6;
};
require_nsb(y,15);