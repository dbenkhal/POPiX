a = 0.25;
b = 5000.0;
n = 25.0;
h = (b - a) / n;
xb = 0.0;
r = 0.0;
u =1000.0;
xa = 0.25;
tmp_1=0.0;
while (xa < 5000.0) {
	v = xa + h;
	if (v > 5000.0) {
		 tmp_1 = 5000.0;
	 } else {
			 tmp_1 = v;};
	xb = tmp_1;
	gxa = u / ((((((0.7 * xa) * xa) * xa) - ((0.6 * xa) * xa)) + (0.9 * xa)) - 0.2);
    gxb = u / ((((((0.7 * xb) * xb) * xb) - ((0.6 * xb) * xb)) + (0.9 * xb)) - 0.2);
	r = r + (((gxa + gxb) * 0.5) * h);
	xa = xa + h;
};
require_nsb(r,14);
