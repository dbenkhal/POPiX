x = 1.0;
y = 1.0;
z = 0.0;
h = 0.1;
t = 0.0;
yn = 0.0;
ff= 0.0;
coeff = 0.16666667;

while(t<1.0){
	ff = 2.0*x + 3.0*y;
	k1 = h*ff;
	p1= x + (h * 0.5);
	p2 = y + (k1 * 0.5);
	p3= 2.0 * p1 + 3.0 * p2;
	k2 = h * p3;
	q1 = y + k2 * 0.5;
	q2 = 2.0 * p1 + 3.0 * q1;
	k3 = h * q2;
	s1= x + h;
	s2 = y +k3;
	s3 = 2.0 * s1 + 3.0 * s2;
	k4 = h * s3;
	yn = y + (coeff* h * (k1+(2.0*k2) +(2.0*k3)+k4));
	t = t + 0.1;
	y = yn;
};
require_nsb(yn, 23);
